            <field name="model">mrp.workcenter.productivity</field>
            <field name="inherit_id" ref="mrp.oee_tree_view"/>
            <field name="arch" type="xml">
                <field name="workcenter_id" position="after">
                    <field name="work_qty"/>
                    <field name="unqa_qty"/>
                    <field name="scrap_qty"/>
